# Optional: Gemini Retry Configuration
# Default: 4 retries, 1 second base delay, 30 second max delay
GEMINI_MAX_RETRIES=4
GEMINI_BASE_DELAY=1000
GEMINI_MAX_DELAY=30000

# Optional: Gemini Timeout Configuration
# Default: 15s connection, 120s request, 45s first token, 10s heartbeat
GEMINI_CONNECTION_TIMEOUT=15000
GEMINI_REQUEST_TIMEOUT=120000
GEMINI_FIRST_TOKEN_TIMEOUT=45000
GEMINI_STREAM_HEARTBEAT_INTERVAL=10000

# Gemini API Configuration
# ========================

# API Key (required)
GEMINI_API_KEY=

# Model Configuration
# ===================
# Model name to use for Gemini API calls
# Default: gemini-3-pro-preview
GEMINI_MODEL=gemini-2.5-pro

# Inference Parameters (optional)
# =================================
# Temperature: Controls randomness in generation (0.0 to 2.0)
# Leave unset to use model defaults
# GEMINI_TEMPERATURE=

# Top P: Nucleus sampling threshold (0.0 to 1.0)
# Leave unset to use model defaults
# GEMINI_TOP_P=

# Top K: Number of top tokens to consider (integer)
# Leave unset to use model defaults
# GEMINI_TOP_K=

# Max Output Tokens: Maximum number of tokens to generate (integer)
# Leave unset to use model defaults
GEMINI_MAX_OUTPUT_TOKENS=65500

# Retry Configuration
# ===================
# Maximum number of retry attempts
GEMINI_MAX_RETRIES=4

# Base delay in milliseconds before first retry
GEMINI_BASE_DELAY=1000

# Maximum delay in milliseconds between retries
GEMINI_MAX_DELAY=30000

# Timeout Configuration
# ====================
# Connection timeout in milliseconds (time to establish connection)
GEMINI_CONNECTION_TIMEOUT=15000

# Request timeout in milliseconds (total time for non-streaming requests)
GEMINI_REQUEST_TIMEOUT=600000

# First token timeout in milliseconds (time to wait for first token in streaming)
GEMINI_FIRST_TOKEN_TIMEOUT=45000

# Stream heartbeat interval in milliseconds (interval to check for stream activity)
GEMINI_STREAM_HEARTBEAT_INTERVAL=10000

# Supabase Configuration
# ======================
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=